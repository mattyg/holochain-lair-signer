{
  "name": "holochain-lair-signer",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattyg/holochain-lair-signer"
  },
  "version": "0.1.1",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "holochain-lair-signer",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-musl",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.17.0",
    "ava": "^5.1.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@4.0.2",
  "optionalDependencies": {
    "holochain-lair-signer-win32-x64-msvc": "0.1.1",
    "holochain-lair-signer-darwin-x64": "0.1.1",
    "holochain-lair-signer-linux-x64-gnu": "0.1.1",
    "holochain-lair-signer-darwin-arm64": "0.1.1",
    "holochain-lair-signer-android-arm64": "0.1.1",
    "holochain-lair-signer-linux-arm64-musl": "0.1.1",
    "holochain-lair-signer-linux-arm-gnueabihf": "0.1.1",
    "holochain-lair-signer-freebsd-x64": "0.1.1",
    "holochain-lair-signer-win32-ia32-msvc": "0.1.1",
    "holochain-lair-signer-android-arm-eabi": "0.1.1",
    "holochain-lair-signer-darwin-universal": "0.1.1"
  }
}
